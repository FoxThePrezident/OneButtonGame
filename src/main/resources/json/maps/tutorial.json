    "interactive": [
      { "position": { "x": 9, "y": 18 }, "text": "Hello adventurer!", "entityType": "sign" },
      { "position": { "x": 9, "y": 16 }, "text": "Welcome to my game, made with passion.", "entityType": "sign" },
      { "position": { "x": 9, "y": 14 }, "text": "Worked on this so much, that is almost a solid demo.", "entityType": "sign" },
      { "position": { "x": 9, "y": 12 }, "text": "But still, there are many things, that needs to be done.", "entityType": "sign" },
      { "position": { "x": 9, "y": 10 }, "text": "You probably noticed, that movement is a little bit weird.", "entityType": "sign" },
      { "position": { "x": 11, "y": 10 }, "text": "It is by a design.", "entityType": "sign" },
      { "position": { "x": 13, "y": 10 }, "text": "You move by pressing ANY key on your keyboard.", "entityType": "sign" },
      { "position": { "x": 15, "y": 10 }, "text": "And your character will then move based on arrow rotation.", "entityType": "sign" },
      { "position": { "x": 15, "y": 8 }, "text": "I just saw a gap in this type of movement.", "entityType": "sign" },
      { "position": { "x": 15, "y": 6 }, "text": "I believe, that there is no other game with movement like this.", "entityType": "sign" },
      { "position": { "x": 15, "y": 4 }, "text": "Originally intended to be on Android, but computer is easier to develop and debug.", "entityType": "sign" },
      { "position": { "x": 15, "y": 2 }, "text": "Now that out of the way, lets start a tutorial.", "entityType": "sign" },
      { "position": { "x": 13, "y": 2 }, "text": "Movement is programmed, like detect any keyboard input and move.", "entityType": "sign" },
      { "position": { "x": 11, "y": 2 }, "text": "Speed of changing arrows can be adjusted in save files.", "entityType": "sign" },
      { "position": { "x": 9, "y": 2 }, "text": "About combat, it is as simple as go on top of the enemy.", "entityType": "sign" },
      { "position": { "x": 7, "y": 2 }, "text": "Same with any other interactive thing, you occur.", "entityType": "sign" },
      { "position": { "x": 5, "y": 2 }, "text": "Now, lets try it, go on top of that zombie.", "entityType": "sign" },
      { "position": { "x": 3, "y": 2 }, "text": "Do not worry, he will not kill you.", "entityType": "sign" },
      { "position": { "x": 3, "y": 7 }, "entityType": "zombie" },
      { "position": { "x": 3, "y": 12 }, "text": "He for now deals 10HP damage to you.", "entityType": "sign" },
      { "position": { "x": 3, "y": 14 }, "text": "About damage, subtraction goes as follows: 1.weapon, 2.armour, 3.you.", "entityType": "sign" },
      { "position": { "x": 3, "y": 16 }, "text": "Now, lets do potions.", "entityType": "sign" },
      { "position": { "x": 3, "y": 20 }, "entityType": "hp" },
      { "position": { "x": 3, "y": 25 }, "text": "This was a HP potion, it gives you 10HP back.", "entityType": "sign" },
      { "position": { "x": 3, "y": 27 }, "text": "It gives you directly, not to your weapon or armour.", "entityType": "sign" },
      { "position": { "x": 3, "y": 29 }, "text": "And that is all for now.", "entityType": "sign" },
      { "position": { "x": 3, "y": 31 }, "text": "If you want, you can leave this level and possibly look at source code.", "entityType": "sign" }
    ],
